{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended","prettier","plugin:node/recommended"],
    "typeof": true,
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
